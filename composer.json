{
    "name": "g4b0rdev/filament-package-skeleton",
    "description": "Generate a new Filament package skeleton",
    "keywords": [
        "laravel",
        "filament",
        "filamentphp",
        "cli",
        "skeleton",
        "filament package development"
    ],
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Gabor"
        }
    ],
    "scripts": {
        "fix": "vendor/bin/duster fix",
        "test": "vendor/bin/pest"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/G4b0rDev/laravel-stub"
        }
    ],
    "require": {
        "php": "^8.2.0",
        "binafy/laravel-stub": "@dev",
        "illuminate/support": "^11.32",
        "laravel-zero/framework": "^11.0.2"
    },
    "require-dev": {
        "laravel/pint": "^1.18.1",
        "mockery/mockery": "^1.6",
        "pestphp/pest-plugin-faker": "^3.0",
        "tightenco/duster": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "App\\Providers\\AppServiceProvider"
            ]
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "pestphp/pest-plugin-faker": true,
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "bin": [
        "filament-package-skeleton"
    ]
}
